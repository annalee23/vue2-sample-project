{"ast":null,"code":"import { mapState } from 'vuex';\nimport moment from 'moment';\nexport default {\n  computed: {\n    ...mapState(['meetingsList']),\n    headers() {\n      return [{\n        text: 'Город',\n        value: 'client_city'\n      }, {\n        text: 'Клиент',\n        value: 'client_name'\n      }, {\n        text: 'Банк',\n        value: 'bank_name'\n      }, {\n        text: 'Продукт',\n        value: 'stg'\n      }, {\n        text: 'Статус',\n        value: 'meeting_state'\n      }, {\n        text: 'Дата встречи',\n        value: 'meeting_date'\n      }, {\n        text: 'Место встречи',\n        value: 'meeting_place'\n      }, {\n        text: 'Телефон',\n        value: 'person_phone'\n      }];\n    }\n  },\n  methods: {\n    formatDate(date) {\n      return moment(date).format('DD.MM.YYYY HH:mm');\n    },\n    statusClass(status) {\n      return {\n        'status-init': status === 'init',\n        'status-error': status === 'error',\n        'status-upload_docs': status === 'upload_docs',\n        'status-process': status === 'process',\n        'status-reupload_fls': status === 'reupload_fls'\n      };\n    }\n  },\n  openOrderDetails({\n    item\n  }) {\n    this.$store.dispatch('fetchOrderDetails', item.id).then(() => {\n      this.$root.$emit('open-order-tab', this.$store.state.currentOrder);\n    }).catch(error => {\n      console.error('Ошибка при загрузке данных заявки:', error);\n    });\n  },\n  mounted() {\n    this.$store.dispatch('fetchMeetingsList');\n  }\n};","map":{"version":3,"names":["mapState","moment","computed","headers","text","value","methods","formatDate","date","format","statusClass","status","openOrderDetails","item","$store","dispatch","id","then","$root","$emit","state","currentOrder","catch","error","console","mounted"],"sources":["src/components/MyComponent2.vue"],"sourcesContent":["<template>\n  <div class=\"mx-1\">\n    <v-toolbar class=\"mt-6\" flat>\n      <h2>Открытие счетов</h2>\n    </v-toolbar>\n    <v-data-table\n      :headers=\"headers\"\n      :items=\"meetingsList\"\n      :items-per-page=\"5\"\n      class=\"elevation-1\"\n    >\n      <template #[`item.stg`]=\"{ item }\">\n        <span>{{ item.stg.join(', ') }}</span>\n      </template>\n      <template #[`item.meeting_date`]=\"{ item }\">\n        <span>{{ formatDate(item.meeting_date) }}</span>\n      </template>\n      <template #[`item.meeting_state`]=\"{ item }\">\n        <span :class=\"statusClass(item.meeting_state)\">{{ item.meeting_state }}</span>\n      </template>\n    </v-data-table>\n  </div>\n</template>\n  \n  <script>\n  import { mapState } from 'vuex';\n  import moment from 'moment';\n\n  \n  export default {\n    computed: {\n      ...mapState(['meetingsList']),\n      headers() {\n        return [\n          { text: 'Город', value: 'client_city' },\n          { text: 'Клиент', value: 'client_name' },\n          { text: 'Банк', value: 'bank_name' },\n          { text: 'Продукт', value: 'stg' },\n          { text: 'Статус', value: 'meeting_state' },\n          { text: 'Дата встречи', value: 'meeting_date' },\n          { text: 'Место встречи', value: 'meeting_place' },\n          { text: 'Телефон', value: 'person_phone' }\n        ];\n      }\n    },\n    methods: {\n    formatDate(date) {\n      return moment(date).format('DD.MM.YYYY HH:mm');\n    },\n    statusClass(status) {\n      return {\n        'status-init': status === 'init',\n        'status-error': status === 'error',\n        'status-upload_docs': status === 'upload_docs',\n        'status-process': status === 'process',\n        'status-reupload_fls': status === 'reupload_fls',\n      };\n    }\n  },\n  openOrderDetails({ item }) {\n      this.$store.dispatch('fetchOrderDetails', item.id)\n        .then(() => {\n          this.$root.$emit('open-order-tab', this.$store.state.currentOrder);\n        })\n        .catch(error => {\n          console.error('Ошибка при загрузке данных заявки:', error);\n        });\n  },\n  mounted() {\n    this.$store.dispatch('fetchMeetingsList');\n  }\n  };\n  </script>\n  \n  <style scoped>\n  \n  h2 {\n    margin-bottom: 20px;\n    padding: 20px;\n    text-align: center;\n  }\n  \n  .status-init {\n    color: #ff6f00; /* Ярко-оранжевый */\n    font-weight: bold;\n    padding: 5px;\n  }\n\n  .status-error {\n    color: #d32f2f; /* Ярко-красный */\n    font-weight: bold;\n    padding: 5px;\n  }\n\n  .status-upload_docs {\n    color: #0288d1; /* Ярко-голубой */\n    font-weight: bold;\n    padding: 5px;\n  }\n\n  .status-process {\n    color: #388e3c; /* Ярко-зеленый */\n    font-weight: bold;\n    padding: 5px;\n  }\n\n  .status-reupload_fls {\n    color: #7b1fa2; /* Ярко-фиолетовый */\n    font-weight: bold;\n    padding: 5px;\n  }\n\n  </style>\n  "],"mappings":"AAyBA,SAAAA,QAAA;AACA,OAAAC,MAAA;AAGA;EACAC,QAAA;IACA,GAAAF,QAAA;IACAG,QAAA;MACA,QACA;QAAAC,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,EACA;IACA;EACA;EACAC,OAAA;IACAC,WAAAC,IAAA;MACA,OAAAP,MAAA,CAAAO,IAAA,EAAAC,MAAA;IACA;IACAC,YAAAC,MAAA;MACA;QACA,eAAAA,MAAA;QACA,gBAAAA,MAAA;QACA,sBAAAA,MAAA;QACA,kBAAAA,MAAA;QACA,uBAAAA,MAAA;MACA;IACA;EACA;EACAC,iBAAA;IAAAC;EAAA;IACA,KAAAC,MAAA,CAAAC,QAAA,sBAAAF,IAAA,CAAAG,EAAA,EACAC,IAAA;MACA,KAAAC,KAAA,CAAAC,KAAA,wBAAAL,MAAA,CAAAM,KAAA,CAAAC,YAAA;IACA,GACAC,KAAA,CAAAC,KAAA;MACAC,OAAA,CAAAD,KAAA,uCAAAA,KAAA;IACA;EACA;EACAE,QAAA;IACA,KAAAX,MAAA,CAAAC,QAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}